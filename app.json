{
  "expo": {
    "name": "파킨슨운동일기",
    "slug": "parkinson_app",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./src/image/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/image/logo.png",
          "color": "#ffffff",
          "sounds": ["./src/sound/mp_ring.mp3"]
        }
      ]
    ],

    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hcclab.helloparkinson"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hcclab.helloparkinson"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
